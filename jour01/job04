class Personne:
    def __init__(self, prenom, nom):
        self.utilisateur_nom = nom
        self.utilisateur_prenom = prenom
        
    def SePresenter(self):
        print("Je suis", self.utilisateur_prenom, self.utilisateur_nom)

presentation1 = Personne("John", "Doe")
presentation2 = Personne("Jean", "Dupond")
presentation1.SePresenter()
presentation2.SePresenter()